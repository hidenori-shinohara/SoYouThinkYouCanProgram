%p#notice= notice
%h1 Listing Solution Submissions
%table
  %thead
    %tr
      %th Solition
      %th{:colspan => "3"}
  %tbody
    - @solution_submissions.each do |solution_submission|
      %tr
        %td= solution_submission.solition
        %td= link_to 'Show', solution_submission
        %td= link_to 'Edit', edit_solution_submission_path(solution_submission)
        %td= link_to 'Destroy', solution_submission, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Solution submission', new_solution_submission_path
