= form_for(@match) do |f|
  - hidden_field_tag(:created_by, "hiiii")
  = f.text_field :created_by
  - if @match.errors.any?
    #error_explanation
      %h2
        = pluralize(@match.errors.count, "error")
        prohibited this match from being saved:
      %ul
        - @match.errors.full_messages.each do |message|
          %li= message
  .actions
    = f.submit
